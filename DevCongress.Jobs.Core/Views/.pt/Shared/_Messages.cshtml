<div class="row">
  @if (TempData.ContainsKey("Successes"))
  {
    foreach (var success in JsonConvert.DeserializeObject<string[]>(TempData["Successes"].ToString()))
    {
      <div class="alert alert-success">
        @*<button type="button" aria-hidden="true" class="close">
            <i class="material-icons">close</i>
          </button>*@
        <span>
          @success
        </span>
      </div>
    }
  }

  @if (TempData.ContainsKey("Information"))
  {
    foreach (var success in JsonConvert.DeserializeObject<string[]>(TempData["Information"].ToString()))
    {
      <div class="alert alert-info">
        @*<button type="button" aria-hidden="true" class="close">
            <i class="material-icons">close</i>
          </button>*@
        <span>
          @success
        </span>
      </div>
    }
  }

  @if (TempData.ContainsKey("Warnings"))
  {
    foreach (var success in JsonConvert.DeserializeObject<string[]>(TempData["Warnings"].ToString()))
    {
      <div class="alert alert-warning">
        @*<button type="button" aria-hidden="true" class="close">
            <i class="material-icons">close</i>
          </button>*@
        <span>
          @success
        </span>
      </div>
    }
  }

  @if (TempData.ContainsKey("Errors"))
  {
    foreach (var error in JsonConvert.DeserializeObject<string[]>(TempData["Errors"].ToString()))
    {
      <div class="alert alert-danger">
        @*<button type="button" aria-hidden="true" class="close">
            <i class="material-icons">close</i>
          </button>*@
        <span>
          @error
        </span>
      </div>
    }
  }
</div>
